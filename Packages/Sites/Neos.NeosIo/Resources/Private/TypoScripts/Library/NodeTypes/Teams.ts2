prototype(Neos.NeosIo:Teams) < prototype(TYPO3.Neos:Content) {
    sectionName = 'Main'
    attributes.class = 'teams'

    teams = ${q(site).crowdGroups().filter('[neos_group_type = team]')}
    guilds = ${q(site).crowdGroups().filter('[neos_group_type = guild]')}

    @cache {
        mode = 'cached'
        entryIdentifier {
            documentNode = ${documentNode}
            node = ${node}
        }
        entryTags {
            1 = ${'Node_' + node.identifier}
        }
    }
}
